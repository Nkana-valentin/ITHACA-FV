FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;
//EigenSolver spectra;

// Number of modes to output and to use for projection
//NmodesUout 50;
NmodesUproj 10;
//NmodePUout 50;
NmodesPproj 10;
//NmodesDout 1;
//NmodesDproj 1;

// Output format to save market vectors.
OutPrecision 20;
OutType fixed;

bcMethod penalty;
warning false;

//debug 1;

// // Inlet boundary conditions velocities.
 online_velocities vel.txt;
POD_pointDisplacement   Frobenius;
//POD_T   Frobenius;

NmodesU  10;
NmodesP  5;
NmodesS  10;
