FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;
//EigenSolver spectra;

// Number of modes to output and to use for projection
NmodesUout 50;
NmodesUproj 5; //15

NmodePUout 50;
NmodesPproj 5; //5?????

NmodesNutOut 50; //50
NmodesNutProj 2; //10 //15

NmodesDout 3;
NmodesDproj 1; //3

debug 1;
// Output format to save market vectors.
OutPrecision 20;
OutType fixed;

//bcMethod penalty;
warning false;
POD_pointDisplacement  Frobenius;// L2;
//POD_DataPoints   Frobenius;
// // Do not export middle fields for this case
epochs 1000; //500

bcMethod      lift; 
