FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      ITHACAdict;
}

// EigenValue solver, can be eigen or spectra
EigenSolver eigen;

// Number of modes to output and to use for projection
NmodesUout 50;
NmodesPout 50;
NmodesUproj 25;
NmodesPproj 25;
//NmodesSUPout 15;
//NmodesSUPproj 10;


// Output format to save market vectors.
OutPrecision 20;
OutType fixed;
//bcMethod penalty;
warning false;

// // Inlet boundary conditions velocities.
 online_velocities vel.txt;
 //POD_U Frobenius;
 //POD_p Frobenius;

